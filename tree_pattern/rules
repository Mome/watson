S  : NP VP . : NP-VP : S

VP : V NP : V-NP : V
VP : V PP : PP-V : V
VP : V ADJP : ADJP-V : V
VP : V NP PP : PP-V, V-NP : V

NP : DT N : DT-N : N
NP : NP1 PP : PP-NP1 : NP1
NP : DT J N : DT-N, J-N : N

PP : Y[label={TO,IN}] NP : NP-Y : Y

DT : _[terminal={a, an}] :: §indefinite
DT : the :: §definite

